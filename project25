def factorial(number):
    if(number==1):
        return 1
    else:
        return factorial(number-1)* number
def sumation(number):
    sum=0
    index=1
    while(index<=number):
        sum+=index
        index+=1
    return(sum)
def divide(number):
    div=0
    index=1
    while(index<=number):
        div+=factorial(index)/sumation(index)
        index+=1
    return div
div=divide(6)
print(div)
